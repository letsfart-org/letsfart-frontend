generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model AccountInfo {
  id                         Int       @id @default(autoincrement())
  account_address            String    @unique @db.VarChar(200)
  pkey                       String    @db.VarChar(200)
  is_used                    Int    @default(0) //0: not use, 1: used

  created_at         DateTime? @default(now()) @db.Timestamp(0)
  updated_at         DateTime? @default(now()) @db.Timestamp(0)

  @@map("mint_account")
}
